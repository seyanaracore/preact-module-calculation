{
  "include": [
    "db/dbHandler.ts",
    "db/requiredIds.json",
    "db/jsonFromCsvCatalog.json",
    "db/catalogLinks.json",
    "db/rawCatalogForProdService.json",
    "./src/types/shims-global.d.ts",
    "./src/types/DBItem.ts",
    "./vite.config.ts"
  ],
  "compilerOptions": {
    "types": ["node"],
    "composite": true,
    "outDir": "dist",
    "baseUrl": "./",
    "esModuleInterop": true,
    "allowImportingTsExtensions": true,
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "noImplicitAny": true,
    "verbatimModuleSyntax": true,
    "emitDeclarationOnly": true,
    "strictPropertyInitialization": true,
    "strictFunctionTypes": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowJs": true,
    "checkJs": true,
    "resolveJsonModule": true,
    "strictNullChecks": true,
    "strict": true,
    "skipLibCheck": true
  }
}
